local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("burger/syn's advertising", "Synapse")

-- main
local Main = Window:NewTab("advertise")
local MainSection = Main:NewSection("syn/burgers advertising")


MainSection:NewButton("Advertise", "Advertise random shit", function()
    while true do wait(7)   local A_1 = "join .gg/pYGayfKJSJ || lol2 scams leave his server" local A_2 = "All"  local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2) end
    
end)

MainSection:NewButton("Anti afk", "Anti afk", function()
wait(3)
local VirtualUser=game:service'VirtualUser'
game:service('Players').LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)

end)

MainSection:NewButton("No cpu usage (1 fps)", "no cpu", function()
    setfpscap(1)
end)
MainSection:NewButton("No cpu usage (no rendering)", "no cpu", function()
    game:GetService("RunService"):Set3dRenderingEnabled(false)
end)

MainSection:NewButton("Un no cpu usage", "un no cpu", function()
    setfpscap(60)
end)

MainSection:NewButton("Un no cpu usage", "un no cpu", function()
    game:GetService("RunService"):Set3dRenderingEnabled(true)
end)
